<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
 
  <groupId>lucas8338.maven_archetypes</groupId>
  <artifactId>simple_general</artifactId>
  <version>1.0.0.1</version>
  <packaging>maven-archetype</packaging>
  
  <description>
	an maven simple and minimal usable archetype which can be used for application or library and can easily be configured
	for other things.
  </description>
 
  <build>
    <extensions>
      <extension>
        <groupId>org.apache.maven.archetype</groupId>
        <artifactId>archetype-packaging</artifactId>
        <version>3.1.1</version>
      </extension>
    </extensions>
  </build>
  
  <distributionManagement>
    <repository>
      <id>github</id>
      <name>GitHub Packages</name>
      <url>https://maven.pkg.github.com/lucas8338/mavenArchetypes</url>
    </repository>
  </distributionManagement>
</project>